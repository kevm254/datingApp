<tr>
  <td><%= member.first_name %></td>
  <td><%= member.last_name %></td>
  <td><%= member.description %></td>
  <td><%= HobbyConstant::HOBBIES.each do |hobbyconst| %>
        <label for="li" class="li"><%= check_box_tag :hobby, :name => hobbyconst %>
          <u><%= hobbyconst %></u></label></td>
  <td><%= link_to 'Show', member %></td>
  <td><%= link_to 'Edit', edit_member_path(member) %></td>
  <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' }, :remote => true  %></td>
  <td><%= image_tag member.avatar.url %></td>
</tr>